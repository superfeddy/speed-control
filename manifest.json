{
  "manifest_version": 2,

  "name": "Speed Control",
  "description": "This extension allows the user to control the speed of HTML5 videos",
  "version": "1.1",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "storage"
  ]
}
